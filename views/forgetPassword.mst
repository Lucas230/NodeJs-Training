{{>header}}

<h2>Mude sua senha</h2>

<form method="POST">
    <label>
        Nova senha:
        <input type="password" name="senha" />
    </label>
    <br />
    <label>
        Confirmar nova senha:
        <input type="password" name="senha-confirma" />
    </label>
    <br />
    <label>
        <input type="submit" value="Salvar" />
    </label>
</form>